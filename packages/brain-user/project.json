{
  "name": "@brain-toolkit/brain-user",
  "sourceRoot": "packages/brain-user/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "packages/brain-user"
      },
      "inputs": [
        "{projectRoot}/src/**/*.ts",
        "{projectRoot}/tsup.config.ts",
        "{projectRoot}/tsconfig.json",
        "{projectRoot}/package.json",
        "{projectRoot}/README.md"
      ],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    }
  }
}
